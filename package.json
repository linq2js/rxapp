{
  "name": "rxapp",
  "version": "0.0.12",
  "description": "The tiny framework for building reactive web app",
  "main": "./core/index.js",
  "scripts": {
    "examples/basic": "parcel examples/basic/index.html --open --out-dir .temp/basic",
    "examples/silky-smooth": "parcel examples/silky-smooth/index.html --open --out-dir .temp/silky-smooth",
    "examples/preact-rainbow": "parcel examples/preact-rainbow/index.html --open --out-dir .temp/preact-rainbow",
    "examples/performance": "parcel examples/performance/index.html --open --out-dir .temp/performance",
    "examples/keyed-list": "parcel examples/keyed-list/index.html --open --out-dir .temp/keyed-list",
    "examples/lazy-render": "parcel examples/lazy-render/index.html --open --out-dir .temp/lazy-render",
    "examples/rainbow": "parcel examples/rainbow/index.html --open --out-dir .temp/rainbow",
    "examples/context": "parcel examples/context/index.html --open --out-dir .temp/context",
    "examples/suspense": "parcel examples/suspense/index.html --open --out-dir .temp/suspense",
    "examples/router": "parcel examples/router/index.html --open --out-dir .temp/router",
    "examples/router-nested": "parcel examples/router-nested/index.html --open --out-dir .temp/router-nested",
    "examples/crypto-search": "parcel examples/crypto-search/index.html --open --out-dir .temp/crypto-search",
    "examples/styled": "parcel examples/styled/index.html --open --out-dir .temp/styled",
    "test": "jest --runInBand --detectOpenHandles",
    "build-browser": "rollup $INPUT --config --file $OUTPUT --output.name rxapp --format iife && gzip-size $OUTPUT",
    "build-browser-core": "cross-env INPUT=./browser/core.js OUTPUT=./dist/core.js npm run build-browser",
    "build-browser-async": "cross-env INPUT=./browser/async.js OUTPUT=./dist/async.js npm run build-browser",
    "build-browser-router": "cross-env INPUT=./browser/router.js OUTPUT=./dist/router.js npm run build-browser",
    "build-browser-styled": "cross-env INPUT=./browser/styled.js OUTPUT=./dist/styled.js npm run build-browser",
    "build-browser-full": "cross-env INPUT=./browser/full.js OUTPUT=./dist/full.js npm run build-browser",
    "build-browser-all": "npm run build-browser-core && npm run build-browser-async && npm run build-browser-router && npm run build-browser-full",
    "_publish": "npm run test && npm run build-browser-all && npm publish"
  },
  "files": [
    "core",
    "router",
    "styled",
    "async",
    "dist"
  ],
  "keywords": [
    "rxapp",
    "reactive",
    "dom",
    "dom manipulation",
    "dom mutating",
    "dom modifier",
    "dom update",
    "html",
    "element",
    "node",
    "frontend",
    "framework",
    "web",
    "app",
    "ui"
  ],
  "alias": {
    "rxapp": "./core"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.7",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-jest": "^26.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "gzip-size-cli": "^3.0.0",
    "history": "^5.0.0",
    "jest": "^26.2.2",
    "less": "^3.12.2",
    "node-fetch": "^2.6.0",
    "parcel-bundler": "^1.12.4",
    "path-to-regexp": "^6.2.0",
    "prettier": "^2.0.5",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "@rollup/plugin-babel": "^5.2.2",
    "stylis": "^4.0.3",
    "typescript": "^4.0.2",
    "uglify-js": "^3.11.5"
  }
}
